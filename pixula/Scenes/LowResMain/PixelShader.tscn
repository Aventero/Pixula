[sub_resource type="GDScript" id="GDScript_vsi1d"]
resource_name = "Grid"
script/source = "@tool
extends TextureRect

@export_tool_button(\"Upload\", \"ColorRect\")
var upload_button : Callable = set_and_fill.bind()

@export var material_type: MainEntry.MaterialType = MainEntry.MaterialType.AIR

@export
var world_viewport : SubViewport

var world_image : Image
var world_texture : ImageTexture

func _ready() -> void:
	set_and_fill()

func set_and_fill() -> void:
	initialize_cells(material_type)
	set_cell(world_image, 0, 0, MainEntry.MaterialType.SAND)
	set_cell(world_image, 1, 1, MainEntry.MaterialType.SAND)

	world_texture.update(world_image)
	material.set_shader_parameter(\"state_texture\", world_texture)

func initialize_cells(cellType : MainEntry.MaterialType) -> void:
	var value: int = cellType
	var grid_size : Vector2i = world_viewport.size
	var packed_byte_array : PackedByteArray = []
	packed_byte_array.resize(grid_size.x * grid_size.y * 4) # 4 bytes in 32 bit uint

	for x in grid_size.x:
		for y in grid_size.y:
			packed_byte_array.encode_u32((y*grid_size.x + x) * 4, value)

	world_image = Image.create_from_data(grid_size.x, grid_size.y, false, Image.FORMAT_RF, packed_byte_array)
	world_texture = ImageTexture.create_from_image(world_image)
	material.set_shader_parameter(\"state_texture\", world_texture)

func set_cell(image: Image, x: int, y: int, cell_type: int) -> void:
	var pba = PackedByteArray()
	pba.resize(4)
	pba.encode_u32(0, cell_type)  # Encode cell_type as uint32
	var float_val = pba.decode_float(0)  # Convert to float for storage
	image.set_pixel(x, y, Color(float_val, 0.0, 0.0, 1.0))  # Store the float

func get_cell(image: Image, x: int, y: int) -> int:
	var pba = PackedByteArray()
	pba.resize(4)

	var float_val = image.get_pixel(x, y).r  # Retrieve float from image
	pba.encode_float(0, float_val)  # Convert back to packed byte array
	return pba.decode_u32(0)  # Decode as uint32
"

